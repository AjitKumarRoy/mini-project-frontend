{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install --legacy-peer-deps",
        "buildCommand": "npm run build",
        "distDir": "dist"
      }
    }
  ],
    "rewrites": [
      { "source": "/:path*", "destination": "/" }
    ] 
  }